{
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "trailingComma": "none",
  "endOfLine": "auto",
  "jsxBracketSameLine": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "printWidth": 65,
  "plugins": ["prettier-plugin-tailwindcss"]
}
