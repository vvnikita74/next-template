{
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"jsxSingleQuote": true,
	"singleQuote": true,
	"arrowParens": "avoid",
	"trailingComma": "none",
	"endOfLine": "auto",
	"jsxBracketSameLine": true,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"cssEnable": ["sass", "scss", "css"],
	"printWidth": 80,
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-classnames"]
}
